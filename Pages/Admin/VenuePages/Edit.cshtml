@page
@model ConFriend.Pages.Admin.VenuePages.EditModel
@{
    ViewData["Title"] = "Edit venue";
}

<h2>Edit venue</h2>

<form method="post">
    <div class="form-group">
        <label asp-for="@Model.Venue.VenueId" class="control-label"></label>
        @Model.Venue.VenueId
    </div>
    <div class="form-group">
        <label asp-for="@Model.Venue.Name" class="control-label"></label>
        <input asp-for="@Model.Venue.Name" class="form-control" />
        <span asp-validation-for="Venue.Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Edit" />
    </div>
</form>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}