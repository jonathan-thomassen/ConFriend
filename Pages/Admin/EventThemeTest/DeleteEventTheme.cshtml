@page
@model ConFriend.Pages.Admin.EventThemeTest.DeleteEventThemeModel
@{
    ViewData["Title"] = "Delete Event-Theme coupling";
}

<h4>Delete Event-Theme coupling</h4>
<form method="post">
    <div class="form-group">
        <label asp-for="@Model.EventTheme.EventId" class="control-label">EventId</label>
        <input asp-for="@Model.EventTheme.EventId" readonly="readonly" class="form-control" />
        <span asp-validation-for="@Model.EventTheme.EventId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.EventTheme.ThemeId" class="control-label">ThemeId</label>
        <input asp-for="@Model.EventTheme.ThemeId" readonly="readonly" class="form-control" />
        <span asp-validation-for="@Model.EventTheme.ThemeId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="delete" class="button btn btn-danger" />
    </div>
</form>