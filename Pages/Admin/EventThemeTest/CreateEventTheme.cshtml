@page
@model ConFriend.Pages.Admin.EventThemeTest.CreateEventThemeModel
@{
    ViewData["Title"] = "Create Event-Theme coupling";
}

<h4>Create Event-Theme coupling</h4>
<form method="post">
    <div class="form-group">
        <label asp-for="@Model.EventTheme.EventId" class="control-label">EventId</label>
        <input asp-for="@Model.EventTheme.EventId" class="form-control" />
        <span asp-validation-for="@Model.EventTheme.EventId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.EventTheme.ThemeId" class="control-label">ThemeId</label>
        <input asp-for="@Model.EventTheme.ThemeId" class="form-control" />
        <span asp-validation-for="@Model.EventTheme.ThemeId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="button btn btn-dark" />
    </div>
</form>