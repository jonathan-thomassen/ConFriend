@page
@model ConFriend.Pages.Admin.EventTest.EditEventModel
@{
    ViewData["Title"] = "Edit event";
}

<h4>Edit Event</h4>
<form method="post">
    <div class="form-group">
        <label asp-for="@Model.NewEvent.EventId" class="control-label">SpeakerID</label>
        <input asp-for="@Model.NewEvent.EventId" readonly="readonly" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.EventId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.SpeakerId" class="control-label">SpeakerID</label>
        <input asp-for="@Model.NewEvent.SpeakerId" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.SpeakerId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.RoomId" class="control-label">RoomId</label>
        <input asp-for="@Model.NewEvent.RoomId" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.RoomId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.ConferenceId" class="control-label">ConferenceId</label>
        <input asp-for="@Model.NewEvent.ConferenceId" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.ConferenceId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.Name" class="control-label">Name</label>
        <input asp-for="@Model.NewEvent.Name" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.StartTime" class="control-label">StartTime</label>
        <input asp-for="@Model.NewEvent.StartTime" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.StartTime" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.Duration" class="control-label">Duration in Minutes</label>
        <input asp-for="@Model.NewEvent.Duration" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.Duration" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.Type" class="control-label">Type</label>
        <input asp-for="@Model.NewEvent.Type" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.Type" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.Description" class="control-label">Description</label>
        <input asp-for="@Model.NewEvent.Description" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.Description" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.Capacity" class="control-label">Capacity</label>
        <input asp-for="@Model.NewEvent.Capacity" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.Capacity" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.Image" class="control-label">Image</label>
        <input asp-for="@Model.NewEvent.Image" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.Image" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.Hidden" class="control-label">Hidden</label>
        <input asp-for="@Model.NewEvent.Hidden" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.Hidden" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.Cancelled" class="control-label">Cancelled</label>
        <input asp-for="@Model.NewEvent.Cancelled" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.Cancelled" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.RoomHidden" class="control-label">RoomHidden</label>
        <input asp-for="@Model.NewEvent.RoomHidden" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.RoomHidden" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.NewEvent.RoomCancelled" class="control-label">RoomCancelled</label>
        <input asp-for="@Model.NewEvent.RoomCancelled" class="form-control" />
        <span asp-validation-for="@Model.NewEvent.RoomCancelled" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="button btn btn-dark" />
    </div>
</form>